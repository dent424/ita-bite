<h1>All users</h1>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user %>
      <% if current_user.admin? && !current_user?(user) %>
      	| <%= link_to t(:delete), user, method: :delete, data: {confirm: "You Sure?"} %>
      <% end %>
    </li>
  <% end %>
</ul>